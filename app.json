{
  "expo": {
    "name": "SettleChip",
    "slug": "SettleChip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-package/iOS/AppIcon.appiconset/Icon-60.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abemiller.settlechip",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-package/Android/mipmap-hdpi/ic_launcher.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abemiller.settlechip"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4152a4d2-4f80-4bd6-a165-d822f418c052"
      }
    }
  }
}
